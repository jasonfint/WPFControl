<UserControl x:Class="ImageButton.ImageB"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageButton"
             mc:Ignorable="d" 
             d:DesignHeight="319" d:DesignWidth="517" ContentTemplate="{DynamicResource DataTemplate1}">
    <UserControl.Resources>
        <DataTemplate x:Key="DataTemplate1">
            <Grid/>
        </DataTemplate>
    </UserControl.Resources>
    <Grid>
        <TextBlock  Style="{DynamicResource ImageButtonStyle}"/>
        <local:ImageBtn/>

    </Grid>
</UserControl>
